"version": 1
"control-service":
   "hostname": "<%= p('flocker.node.agent.control_service.hostname') %>"
   "port": 4524

# The dataset key below selects and configures a dataset backend (see below: aws/openstack/etc).
# All nodes will be configured to use only one backend

dataset:
  backend: "<%= p('flocker.node.agent.dataset.backend') %>"
  region: "<%= p('flocker.node.agent.dataset.region') %>"
  auth_plugin: "<%= p('flocker.node.agent.dataset.auth_plugin') %>"
  auth_url: <%= p('flocker.node.agent.dataset.auth_url') %> #
  tenant_name: <%= p('flocker.node.agent.dataset.tenant_name') %>
  username: <%= p('flocker.node.agent.dataset.username') %>
  password: <%= p('flocker.node.agent.dataset.password') %> 
  verify_peer: false
    
